{
  "name": "levgaas",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "e2e": "ng e2e",
    "hmr": "ng serve --hmr -e=hmr",
    "build": "ng build --prod",
    "test": "ng test",
    "coveralls:cat": "cat ./coverage/apps/angular-tuned/lcov.info ./coverage/libs/elements/lcov.info ./coverage/libs/material-theming/lcov.info > coverage.info",
    "coveralls:transform": "./node_modules/.bin/coveralls-lcov -v -n coverage.info > coverage.json",
    "coveralls": "cat ./coverage/apps/angular-tuned/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "./node_modules/.bin/nx lint && ./node_modules/.bin/stylelint src/**/*.scss && ng lint",
    "fix": "./node_modules/.bin/stylelint src/**/*.scss --fix && ng lint --fix",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "cz-commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "docker-clear-web-container": "docker stop angular-tuned && docker rm angular-tuned",
    "docker-clear-web-image": "docker rmi nxproduction_web",
    "docker-clear-web": "npm run clear-web-container && npm run clear-web-image",
    "angular-tuned-build-prod-for-docker": "ng run angular-tuned:build:production && gzip -r dist/*",
    "angular-tuned-serve-for-docker": "npm run angular-tuned-build-prod-for-docker && docker-compose up"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/elements": "^6.1.2",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.1",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@angular/service-worker": "^6.1.0",
    "@levgaas/material-theming": "file:libs/material-theming",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.0.1",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@nrwl/nx": "6.2.0",
    "@nrwl/schematics": "^7.7.0",
    "angularfire2": "^5.0.0-rc.11",
    "compression": "^1.7.3",
    "core-js": "^2.5.7",
    "firebase": "^5.3.0",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26",
    "document-register-element": "^1.11.0"
  },
  "devDependencies": {
    "ngrx-store-freeze": "0.2.4",
    "@nrwl/schematics": "6.2.0",
    "jasmine-marbles": "0.3.1",
    "prettier": "1.13.7",
    "@angular-devkit/build-angular": "^0.7.1",
    "@angular/cli": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "@commitlint/prompt": "^7.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "commitizen": "^2.10.1",
    "cpy-cli": "^2.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "express": "^4.16.3",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.2.0",
    "protractor": "~5.3.0",
    "reflect-metadata": "^0.1.12",
    "rxjs-tslint-rules": "^4.7.0",
    "stylelint": "^9.4.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.2.0",
    "ts-loader": "^4.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.0",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "ng-packagr": "^3.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0"
  },
  "lint-staged": {
    "*.ts": [
      "ng lint",
      "git add"
    ],
    "*.scss": "stylelint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
